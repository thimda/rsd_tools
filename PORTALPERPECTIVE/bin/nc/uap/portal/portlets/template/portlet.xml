<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<portlet-app version="2.0" xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd">
	<portlet>
		<description>jspPortlet</description>
		<portlet-name>jspPortlet</portlet-name>
		<display-name>jspPortlet模板</display-name>
		<portlet-class>
			nc.uap.portlet.base.JspPortlet
		</portlet-class>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>VIEW</portlet-mode>
		</supports>
		<portlet-preferences>
			<preference>
				<name>if_src</name>
				<description>链接地址</description>
				<value><![CDATA[/portal/pages/view.jsp]]></value>
				<read-only>false</read-only>
			</preference>
			<preference>
				<name>if_height</name>
				<value>0</value>
				<read-only>false</read-only>
			</preference>
			<preference>
				<name>if_width</name>
				<value>100%</value>
				<read-only>false</read-only>
			</preference>
			<preference>
				<name>version</name>
				<value>1</value>
				<read-only>true</read-only>
			</preference>
		</portlet-preferences>
	</portlet>
	
	<portlet>
		<description>iframePortlet</description>
		<portlet-name>iframePortlet</portlet-name>
		<display-name>iframePortlet模板</display-name>
		<portlet-class>
			nc.uap.portlet.iframe.BaseIframePortlet
		</portlet-class>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>VIEW</portlet-mode>
		</supports>
		<portlet-preferences>
			<preference>
				<name>version</name>
				<value>1</value>
				<read-only>true</read-only>
			</preference>
			<preference>
				<name>if_height</name>
				<value>0</value>
				<read-only>false</read-only>
			</preference>
			<preference>
				<name>if_width</name>
				<value>100%</value>
				<read-only>false</read-only>
			</preference>
			<preference>
				<name>if_src</name>
				<description>链接地址</description>
				<value><![CDATA[]]></value>
				<read-only>false</read-only>
			</preference>
		</portlet-preferences>
	</portlet>
	<portlet>
		<description>浮动链接组Portlet</description>
		<portlet-name>floatFuncPortlet</portlet-name>
		<display-name>浮动链接组Portlet</display-name>
		<portlet-class>
			nc.uap.portal.portlets.linktree.FloatLinkTreePortlet
		</portlet-class>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>VIEW</portlet-mode>
		</supports>
		<portlet-preferences>
			<preference>
				<name>linkgroup</name>
				<value></value>
				<read-only>false</read-only>
				<description>链接组编码</description>
			</preference>
			<preference>
				<name>version</name>
				<value>1</value>
				<read-only>true</read-only>
			</preference>
		</portlet-preferences>
		<portlet-info>
			<title>浮动链接组Portlet</title>
		</portlet-info>
		<supported-publishing-event>
			<name>switchIframeContentEvent</name>
		</supported-publishing-event>
		<supported-processing-event>
			<name>execScriptEvent</name>
		</supported-processing-event>
	</portlet>
</portlet-app>
